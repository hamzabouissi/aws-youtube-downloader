          {'duration': 2.64,
           'start': 412.68,
           'text': 'until something interacts\nwith the particle,',
           'videoId': 'BLqk7uaENAY'},
          {'duration': 1.77,
           'start': 415.32,
           'text': 'which could be an observer',
           'videoId': 'BLqk7uaENAY'},
