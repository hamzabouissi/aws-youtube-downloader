          {'duration': 1.31,
           'start': 421.663,
           'text': '"Stai ancora crescendo."',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 3.852,
           'start': 423.16,
           'text': '"Quando avrai vissuto ogni singola vita umana in ogni '
                   'tempo"',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 2.714,
           'start': 427.012,
           'text': '"sarai cresciuto abbastanza da poter nascere."',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 1.82,
           'start': 430.98,
           'text': '-"Perciò l\'intero Universo..."',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 1.18,
           'start': 433.3,
           'text': '"...è solo..."',
