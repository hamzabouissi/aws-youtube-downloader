          {'duration': 1.58,
           'start': 54.92,
           'text': 'Estaba la nada.',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 2.28,
           'start': 56.74,
           'text': 'Solo tú y yo.',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 1.63,
           'start': 59.635,
           'text': '"¿Qué es este lugar?"',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 2.26,
           'start': 62.26,
