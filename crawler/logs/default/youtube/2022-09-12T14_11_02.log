 'text': [{'duration': 3.3,
           'start': 2.74,
           'text': 'Tu retournais chez toi quand tu es mort.',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 2.0,
           'start': 6.04,
           'text': 'C’était un accident de voiture.',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 2.0,
           'start': 9.06,
           'text': "Rien d'extraordinaire,",
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 2.04,
           'start': 11.28,
           'text': 'mais quand même fatal.',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 1.64,
           'start': 13.62,
           'text': "C'était une mort sans douleur",
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 2.2,
           'start': 15.68,
           'text': 'Les ambulanciers ont tout essayé pour te sauver,',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 1.28,
           'start': 18.24,
           'text': 'mais sans succès.',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 2.62,
           'start': 19.9,
           'text': 'Ton corps était dans un tel état',
           'videoId': 'h6fcK_fRYaI'},
          {'duration': 2.0,
           'start': 22.52,
